.PP
.PP
This header file contains bit mask definitions for use with the AVR TWI interface\&. 
.SH "Define Documentation"
.PP 
.SS "#define \fBTW_BUS_ERROR\fP   0x00"illegal start or stop condition 
.SS "#define \fBTW_MR_ARB_LOST\fP   0x38"arbitration lost in SLA+R or NACK 
.SS "#define \fBTW_MR_DATA_ACK\fP   0x50"data received, ACK returned 
.SS "#define \fBTW_MR_DATA_NACK\fP   0x58"data received, NACK returned 
.SS "#define \fBTW_MR_SLA_ACK\fP   0x40"SLA+R transmitted, ACK received 
.SS "#define \fBTW_MR_SLA_NACK\fP   0x48"SLA+R transmitted, NACK received 
.SS "#define \fBTW_MT_ARB_LOST\fP   0x38"arbitration lost in SLA+W or data 
.SS "#define \fBTW_MT_DATA_ACK\fP   0x28"data transmitted, ACK received 
.SS "#define \fBTW_MT_DATA_NACK\fP   0x30"data transmitted, NACK received 
.SS "#define \fBTW_MT_SLA_ACK\fP   0x18"SLA+W transmitted, ACK received 
.SS "#define \fBTW_MT_SLA_NACK\fP   0x20"SLA+W transmitted, NACK received 
.SS "#define \fBTW_NO_INFO\fP   0xF8"no state information available 
.SS "#define \fBTW_READ\fP   1"SLA+R address 
.SS "#define \fBTW_REP_START\fP   0x10"repeated start condition transmitted 
.SS "#define \fBTW_SR_ARB_LOST_GCALL_ACK\fP   0x78"arbitration lost in SLA+RW, general call received, ACK returned 
.SS "#define \fBTW_SR_ARB_LOST_SLA_ACK\fP   0x68"arbitration lost in SLA+RW, SLA+W received, ACK returned 
.SS "#define \fBTW_SR_DATA_ACK\fP   0x80"data received, ACK returned 
.SS "#define \fBTW_SR_DATA_NACK\fP   0x88"data received, NACK returned 
.SS "#define \fBTW_SR_GCALL_ACK\fP   0x70"general call received, ACK returned 
.SS "#define \fBTW_SR_GCALL_DATA_ACK\fP   0x90"general call data received, ACK returned 
.SS "#define \fBTW_SR_GCALL_DATA_NACK\fP   0x98"general call data received, NACK returned 
.SS "#define \fBTW_SR_SLA_ACK\fP   0x60"SLA+W received, ACK returned 
.SS "#define \fBTW_SR_STOP\fP   0xA0"stop or repeated start condition received while selected 
.SS "#define \fBTW_ST_ARB_LOST_SLA_ACK\fP   0xB0"arbitration lost in SLA+RW, SLA+R received, ACK returned 
.SS "#define \fBTW_ST_DATA_ACK\fP   0xB8"data transmitted, ACK received 
.SS "#define \fBTW_ST_DATA_NACK\fP   0xC0"data transmitted, NACK received 
.SS "#define \fBTW_ST_LAST_DATA\fP   0xC8"last data byte transmitted, ACK received 
.SS "#define \fBTW_ST_SLA_ACK\fP   0xA8"SLA+R received, ACK returned 
.SS "#define \fBTW_START\fP   0x08"start condition transmitted 
.SS "#define \fBTW_STATUS\fP   (TWSR & \fBTW_STATUS_MASK\fP)"TWSR, masked by TW_STATUS_MASK 
.SS "#define \fBTW_STATUS_MASK\fP"\fBValue:\fP
.PP
